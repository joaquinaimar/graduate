<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.demo.hibernate.beans">

    <class name="Meeting" table="meeting">
        <id name="id" column="ID" type="java.lang.Integer">
            <generator class="native"/>
        </id>
 
        <property name="sender" column="sender" type="java.lang.String"  not-null="true" />
        <property name="starttime" column="starttime" type="java.lang.String" />
        <property name="endtime" column="endtime" type="java.lang.String" />
        <property name="address" column="address" type="java.lang.String" />
        <property name="title" column="title" type="java.lang.String" />
        <property name="content" column="content" type="java.lang.String" />
    </class>
    
</hibernate-mapping>
